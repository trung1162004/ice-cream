@model Project.Models.BookCombined

@{
    ViewData["Title"] = "Detail";
    Layout = "_LayoutAdmin";
}
<header>
    <link rel="stylesheet" href="~/css/AdminRecipe.css" />
</header>
<h1>Order Detail</h1>
<p>@ViewBag.msg</p>
<div>
 
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Order!.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Order!.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Book!.Images)
        </dt>
        <dd class="col-sm-10">

            <img src="~/@Model.Book!.Images" width="120" />
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Order!.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Order!.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Order!.Phone)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Order!.Phone)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Order!.Address)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Order!.Address)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Order!.Status)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Order!.Status)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="BookTracking">Back to List</a>
  
</div>


