@model Project.Models.Feedback

@{
    ViewData["Title"] = "Detail";
    Layout = "_LayoutAdmin";
}
<header>
    <link rel="stylesheet" href="~/css/AdminRecipe.css" />
</header>
<h1>Detail Ice Cream Feedback</h1>
<p>@ViewBag.msg</p>
<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Email)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FeedbackText)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FeedbackText)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.FeedbackDate)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FeedbackDate)
        </dd>
        

    </dl>
</div>
<div>
    <a asp-action="DisplayFeedback" class="btn btn-primary">Back to List</a>

</div>
